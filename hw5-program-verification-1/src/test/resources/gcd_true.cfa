main process cfa {
	var a : int
	var b : int

	init loc LI
	loc L0
	loc L1
	final loc LF
	error loc LE

	LI -> L0{
		assume a > 0
		assume b > 0
	}
	L0 -> L0 {
		assume a > b
		a := a - b
	}
	L0 -> L0 {
		assume a < b
		b := b - a
	}
	L0 -> L1 {
		assume a = b
	}
	L1 -> LF {
		assume a /= 0
	}
	L1 -> LE {
		assume a = 0
	}
}
